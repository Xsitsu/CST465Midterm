@model List<TestQuestion>

@{
    ViewData["Title"] = "DisplayResults";
}

<h2>DisplayResults</h2>

@{
    QuestionBank questionBank = new Midterm.Controllers.MidtermController().GetList();

    <p>Count: @Model.Count</p>
    for (int i = 0; i < Model.Count; i++)
    {
        <div>
            @Html.Partial("QuestionHeader", Model[i]);
            <p>@Model[i].Answer</p>
        </div>
    }
}